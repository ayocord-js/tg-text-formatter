{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "eqeqeq": "error",
    "no-var": "error",
    "prefer-const": "error",
    "strict": ["error", "global"],
    "camelcase": "warn",
    "no-undef": "error"
  }
}
